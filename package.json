{
  "name": "typed-ls",
  "license": "MIT",
  "version": "0.1.3",
  "description": "Type-safe local storage",
  "keywords": [
    "local storage",
    "typed",
    "typescript",
    "type-safe"
  ],
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sajmoni/typed-ls.git"
  },
  "scripts": {
    "build": "rm -rf dist && tsc",
    "release": "npm run build && np --no-tests",
    "test": "vitest --dom",
    "qa": "tsc && one"
  },
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "prettier": {
    "semi": false,
    "trailingComma": "all",
    "singleQuote": true
  },
  "devDependencies": {
    "eslint": "8.56.0",
    "happy-dom": "12.10.3",
    "np": "9.2.0",
    "one-eslint": "0.0.4",
    "prettier": "3.1.1",
    "tsconfig-one": "0.0.8",
    "typescript": "5.3.3",
    "vitest": "1.1.0"
  }
}
